       	<!--GOOGLE MAP SCRIPT-->
	<script>
	 "use strict";
        var map1, map2;
        //var imageSrc = document.getElementById("imageSrc").value
        
		// modal initMap			
		$('#locationMap').on('shown.bs.modal', function () {	
		    map2 = map('location-Map');
            map2.initMap(locations);
		})

        // modal click
        $('.location-map').on( "click", function() {
			$('#locationMap').modal('show');
        });

    var locations = [
     {% for item in node.field_resort_map %}
        ["{{ item.entity.field_resort_name.value }}","{{ item.entity.field_activities_type.value }}",{{ item.entity.field_resort_latitude.value }},{{ item.entity.field_resort_longitude.value }}],
    {% endfor %}
    ];

    initMap(locations); //AJAX callback

    function initMap(locations) {
      var script = document.createElement('script');
        script.src = 'https://maps.googleapis.com/maps/api/js?key={{ node.field_location_map_google_map_ap.value }}';

      script.onload = function () {
        map1 = map('map');
        map1.initMap(locations);
      };

      var head = document.getElementsByTagName('head')[0];
      head.appendChild(script);
    }

    //function showIcon(type, mapName) {
      //mapName.setMarker(type);
    //}

    function map(id) {
      var markers = [];
      var staticMarkers = [];
      var map;
      var focused = true;
      var filter = 'all';
      var idMap = id;
      var iconSrc = {
        entertainment: '{{ theme_path }}/dest/images/map-markers/entertainment-pin.svg',
        shopping: '{{ theme_path }}/dest/images/map-markers/shopping-pin.svg',
        resort: '{{ theme_path }}/dest/images/map-markers/resort-pin.svg',
        outdoor: '{{ theme_path }}/dest/images/map-markers/outdoor-pin.svg',
        dining: '{{ theme_path }}/dest/images/map-markers/dining-pin.svg'
      };

      function initMap(locations) {
        var myLatLng = {
           lat: {{ node.field_location_map_latitude.value }},
            lng: {{ node.field_location_map_longitude.value }}
        };
        map = new google.maps.Map(document.getElementById(idMap), {
          zoom: 16,
          center: myLatLng
        });
        map.addListener('zoom_changed', function () {
          var zoom = map.getZoom();

          if (zoom < 16) {
            focused = false;
            hideAll();
          } else {
            focused = true;
            showAll();
          }
        });
        locations.forEach(function (element) {
          if (element[1] === 'resort') {
            staticMarkers.push(new google.maps.Marker({
              position: {
                lat: element[2],
                lng: element[3]
              },
              map: map,
              title: element[0],
              icon: iconSrc[element[1]],
              type: element[1]
            }));
          } else {
            markers.push(new google.maps.Marker({
              position: {
                lat: element[2],
                lng: element[3]
              },
              map: map,
              title: element[0],
              icon: iconSrc[element[1]],
              type: element[1]
            }));
          }
        });
      }

      function setMarker(type) {
        filter = type;

        if (!focused) {
          return;
        }

        markers.forEach(function (marker) {
          if (type === 'all') {
            filter = 'all';
            marker.setVisible(true);
            return;
          }

          if (marker.type !== type) {
            marker.setVisible(false);
          } else {
            marker.setVisible(true);
          }
        });
      }

      function showAll() {
        staticMarkers.forEach(function (marker) {
          marker.setVisible(true);
        });
        markers.forEach(function (marker) {
          if (filter === 'all') {
            filter = 'all';
            marker.setVisible(true);
            return;
          }

          if (marker.type === filter) {
            marker.setVisible(true);
          }
        });
      }

      function hideAll() {
        staticMarkers.forEach(function (marker) {
          marker.setVisible(false);
        });
        markers.forEach(function (marker) {
          marker.setVisible(false);
        });
      }

      return {
        setMarker: setMarker,
        initMap: initMap
      };
    };
	</script>	
